<script src="http://api-maps.yandex.ru/2.0/?load=package.full&mode=debug&lang=ru-RU"
    type="text/javascript"></script>
<script type="text/javascript">

    var map = null;

    /* При успешной загрузке API выполняется
    соответствующая функция */
    ymaps.ready(function () {
        // Инициализация яндекс карт
        map = new ymaps.Map("map", {
            center: [48.534243, 135.177541],
            zoom: 14,
            type: "yandex#map"
        });

        var khvMarker = new ymaps.Placemark([48.534243, 135.177541], { balloonContent: "Матвеевское шоссе 41" }, { preset: "twirl#greenDotIcon" });
        map.geoObjects.add(khvMarker);

        map.controls.add("mapTools").add("zoomControl").add("typeSelector");
    });

    $(".sub-object-gallery-item img").click(function () {
        var idx = $(this).parent().index()-1;
        var items = [];
        $(this).parent().parent().find("img").each(function (index, item) {
            items.push({ href: $(item).attr("src").replace("small/", "") });
        });
        $.fancybox.open(items, {index: idx});
    });
    $("#izoplan img").click(function () {
        $.fancybox.open({ href: $(this).attr("src").replace("small/", "") });
    });
    
    
</script>
